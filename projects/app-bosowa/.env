# ---------------------------------------------------------
# projects/app-bosowa/env.example
# Environment variables untuk App Bosowa
# ---------------------------------------------------------

# App Configuration
APP_NAME=app-bosowa
APP_VERSION=1.0.0
APP_ENV=production

# PHP Configuration
PHP_VERSION=8.3
PHP_MEMORY_LIMIT=512M
PHP_UPLOAD_MAX_FILESIZE=64M
PHP_POST_MAX_SIZE=64M
PHP_MAX_EXECUTION_TIME=120

# MySQL Configuration
MYSQL_VERSION=8.0
MYSQL_ROOT_PASSWORD=supersecretroot
MYSQL_DATABASE=app_bosowa
MYSQL_USER=app_bosowa
MYSQL_PASSWORD=supersecretuser
MYSQL_PORT=3307

# Redis Configuration (Shared)
REDIS_HOST=shared-redis
REDIS_PORT=6379

# Nginx Configuration (Shared)
NGINX_HOST=shared-nginx
NGINX_PORT=80

# Domain Configuration
SERVER_NAME=app-bosowa.localhost
SERVER_EMAIL=admin@app-bosowa.localhost

# Timezone
TZ=Asia/Jakarta
